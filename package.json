{
  "name": "dubular-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "bash -ceu 'opennextjs-cloudflare build --skipWranglerConfigCheck --yes; rm -rf .vercel && mkdir -p .vercel/output; cp -R .open-next/. .vercel/output/; cp -f .open-next/_worker.js .open-next/_routes.json .vercel/output/ || true; rm -f .vercel/output/static/index.html || true'",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "latest",
    "@supabase/supabase-js": "2.57.4",
    "hls.js": "1.6.12",
    "lucide-react": "^0.544.0",
    "next": "^15.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.0",
    "@types/react": "^19.1.13",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  }
}
