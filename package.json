{
  "name": "dubular-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "cleanup": "node scripts/cleanup-ui.cjs",
    "typecheck": "tsc --noEmit",
    "imports": "unimported --flow silent",
    "deps": "depcheck --skip-missing=true",
    "semgrep": "semgrep --quiet --config ./.semgrep.yml --error",
    "spell": "cspell \"**/*\" --no-progress --no-summary --config .cspell.json",
    "prettier:check": "prettier --check .",
    "scan": "npm run lint && npm run typecheck && npm run imports && npm run deps && npm run semgrep && npm run spell && npm run prettier:check",
    "scan:all": "eslint . --ext .js,.jsx,.ts,.tsx && prettier --check . && tsc --noEmit || true",
    "dev:wasm": "NEXT_DISABLE_SWC_BINARY=1 NEXT_FORCE_WASM=1 NEXT_SKIP_NAPI_BINARY_DOWNLOAD=1 next dev -p 3000",
    "preview": "wrangler pages dev .vercel/output/static --compatibility-date=2024-09-01",
    "cf:build": "npx @cloudflare/next-on-pages@latest",
    "cf:deploy": "npx wrangler pages deploy .vercel/output/static --project-name dubular-site"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "hls.js": "^1.6.12",
    "lucide-react": "^0.454.0",
    "next": "^15.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^8.57.1",
    "@next/swc-wasm-nodejs": "^15.5.3",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@swc/wasm": "^1.13.5",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.20",
    "chokidar-cli": "^3.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.32",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.4.0",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.13",
    "typescript": "^5.9.2",
    "unimported": "^1.31.0"
  },
  "type": "module"
}
